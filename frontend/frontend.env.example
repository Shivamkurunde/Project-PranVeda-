# PranVeda Zen Flow - Frontend Environment Variables (EXAMPLE)
# Copy this file to frontend.env and fill in your actual values
# Variables with VITE_ prefix are exposed to the browser - only use public keys here

# =============================================================================
# SUPABASE CONFIGURATION (Database)
# =============================================================================
# Get these from: https://supabase.com/dashboard/project/uctzjcgttgkwlwcvtqnt/settings/api
VITE_SUPABASE_URL=https://uctzjcgttgkwlwcvtqnt.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# =============================================================================
# FIREBASE CONFIGURATION (Authentication)
# =============================================================================
# Get these from: https://console.firebase.google.com/  Project Settings  General
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=907165773979
VITE_FIREBASE_APP_ID=1:907165773979:web:26bd23ea3002e4ebfa963e
VITE_FIREBASE_MEASUREMENT_ID=G-B1CD4T1GMK

# =============================================================================
# GOOGLE OAUTH CONFIGURATION
# =============================================================================
# Get this from: https://console.cloud.google.com/  APIs & Services  Credentials
# Make sure to add http://localhost:8082 to authorized origins
VITE_GOOGLE_CLIENT_ID=your-google-client-id

# =============================================================================
# API CONFIGURATION
# =============================================================================
# Backend API URL (where your Node.js/Express server runs)
VITE_API_URL=http://localhost:5000

# Frontend URL (for CORS and OAuth redirects)
VITE_FRONTEND_URL=http://localhost:8082

# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================
VITE_APP_NAME=PranVeda Zen Flow
VITE_APP_VERSION=1.0.0

# =============================================================================
# OPTIONAL: AI SERVICE KEYS (if using client-side AI)
# =============================================================================
# Only add these if you're using AI services directly in the frontend
# For security, prefer using AI services through your backend API
VITE_GEMINI_API_KEY=your-gemini-key
VITE_DEEPSEEK_API_KEY=your-deepseek-key

# =============================================================================
# DEVELOPMENT CONFIGURATION
# =============================================================================
# Enable debug mode for development
VITE_DEBUG=true

# Enable detailed logging
VITE_LOG_LEVEL=debug

# Enable development tools
VITE_DEV_TOOLS=true

# =============================================================================
# FEATURE FLAGS
# =============================================================================
# Enable/disable specific features during development
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_PERFORMANCE_MONITORING=false

# =============================================================================
# AUDIO CONFIGURATION
# =============================================================================
# Audio file paths and settings
VITE_AUDIO_BASE_URL=/audio
VITE_DEFAULT_VOLUME=0.7
VITE_AUDIO_CACHE_ENABLED=true

# =============================================================================
# THEME CONFIGURATION
# =============================================================================
# Default theme settings
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true
